<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlet2Servlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>index1.html</welcome-file>
    
  </welcome-file-list>
  
  <servlet>
  		<servlet-name>login</servlet-name>
  		<servlet-class>com.akhil.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>login</servlet-name>
  		<url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
  		<servlet-name>plus</servlet-name>
  		<servlet-class>com.akhil.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>plus</servlet-name>
  		<url-pattern>/add</url-pattern>
  </servlet-mapping>

  <servlet>
  		<servlet-name>into</servlet-name>
  		<servlet-class>com.akhil.MultiplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>into</servlet-name>
  		<url-pattern>/multiply</url-pattern>
  </servlet-mapping>

  <servlet>
  		<servlet-name>add2</servlet-name>
  		<servlet-class>com.akhil.Add2Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>add2</servlet-name>
  		<url-pattern>/add2</url-pattern>
  </servlet-mapping>

  <servlet>
  		<servlet-name>db</servlet-name>
  		<servlet-class>com.akhil.DBServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>db</servlet-name>
  		<url-pattern>/dbservlet</url-pattern>
  </servlet-mapping>
  
  
<servlet>
<servlet-name>readAllHeaders</servlet-name>
<servlet-class>com.akhil.DisplayHeadersServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>readAllHeaders</servlet-name>
<url-pattern>/readAllHeaders</url-pattern>
</servlet-mapping>

<servlet>
  		<servlet-name>db1</servlet-name>
  		<servlet-class>com.akhil.DBServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>db1</servlet-name>
  		<url-pattern>/dbservlet1</url-pattern>
  </servlet-mapping>
  </web-app>